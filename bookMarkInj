javascript:(function(){if(document.getElementById('injector-container')){return;}var container=document.createElement('div');container.id='injector-container';container.style.position='fixed';container.style.top='10px';container.style.right='10px';container.style.zIndex='10000';container.style.backgroundColor='white';container.style.border='1px solid #ccc';container.style.padding='10px';container.style.boxShadow='0px 0px 10px rgba(0,0,0,0.1)';container.style.fontSize='14px';var select=document.createElement('select');var options=['HTML','CSS','JavaScript'];options.forEach(function(option){var opt=document.createElement('option');opt.value=option.toLowerCase();opt.innerText=option;select.appendChild(opt);});select.style.marginBottom='10px';container.appendChild(select);var input=document.createElement('textarea');input.style.width='300px';input.style.height='100px';input.style.marginBottom='10px';input.placeholder='Enter code here...';container.appendChild(input);var iframeInput=document.createElement('input');iframeInput.type='text';iframeInput.placeholder='Optional: Enter iframe ID';iframeInput.style.marginBottom='10px';iframeInput.style.display='block';container.appendChild(iframeInput);var executeBtn=document.createElement('button');executeBtn.innerText='Inject Code';executeBtn.style.display='block';executeBtn.style.marginBottom='5px';container.appendChild(executeBtn);var closeBtn=document.createElement('button');closeBtn.innerText='Close';container.appendChild(closeBtn);document.body.appendChild(container);function getTargetDocument(){var iframeID=iframeInput.value.trim();if(iframeID){var iframe=document.getElementById(iframeID);if(iframe&&iframe.contentDocument){return iframe.contentDocument;}else{alert('Iframe not found or inaccessible');return null;}}return document;}executeBtn.onclick=function(){var codeType=select.value;var code=input.value;var targetDocument=getTargetDocument();if(!targetDocument)return;try{if(codeType==='html'){var div=targetDocument.createElement('div');div.innerHTML=code;targetDocument.body.appendChild(div);}else if(codeType==='css'){var style=targetDocument.createElement('style');style.type='text/css';style.appendChild(targetDocument.createTextNode(code));targetDocument.head.appendChild(style);}else if(codeType==='javascript'){var script=targetDocument.createElement('script');script.type='text/javascript';script.appendChild(targetDocument.createTextNode(code));targetDocument.body.appendChild(script);}input.value='';}catch(e){alert('Error injecting code: '+e.message);}};closeBtn.onclick=function(){document.body.removeChild(container);};})();
