javascript:(function(){if(document.getElementById('heading-list-container')){return;}var container=document.createElement('div');container.id='heading-list-container';container.style.position='fixed';container.style.top='10px';container.style.right='10px';container.style.zIndex='10000';container.style.backgroundColor='white';container.style.border='1px solid #ccc';container.style.padding='10px';container.style.maxHeight='300px';container.style.overflowY='auto';container.style.boxShadow='0px 0px 10px rgba(0,0,0,0.1)';container.style.fontSize='14px';var iframeInput=document.createElement('input');iframeInput.type='text';iframeInput.placeholder='Optional: Enter iframe ID';iframeInput.style.marginBottom='10px';iframeInput.style.display='block';container.appendChild(iframeInput);var listBtn=document.createElement('button');listBtn.innerText='List Headings';listBtn.style.display='block';listBtn.style.marginBottom='10px';container.appendChild(listBtn);var closeBtn=document.createElement('button');closeBtn.innerText='Close';container.appendChild(closeBtn);document.body.appendChild(container);function getTargetDocument(){var iframeID=iframeInput.value.trim();if(iframeID){var iframe=document.getElementById(iframeID);if(iframe&&iframe.contentDocument){return iframe.contentDocument;}else{alert('Iframe not found or inaccessible');return null;}}return document;}listBtn.onclick=function(){var targetDocument=getTargetDocument();if(!targetDocument)return;var headings=targetDocument.querySelectorAll('h1,h2,h3,h4,h5,h6');var list=document.createElement('ul');headings.forEach(function(heading){var item=document.createElement('li');item.innerText=heading.tagName+' - '+heading.innerText;list.appendChild(item);});container.appendChild(list);};closeBtn.onclick=function(){document.body.removeChild(container);};})();
