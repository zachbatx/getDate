javascript:(function(){if(document.getElementById('heading-list-container')){return;}var container=document.createElement('div');container.id='heading-list-container';container.style.position='fixed';container.style.top='10px';container.style.right='10px';container.style.zIndex='10000';container.style.backgroundColor='white';container.style.border='1px solid #ccc';container.style.padding='10px';container.style.maxHeight='300px';container.style.overflowY='auto';container.style.boxShadow='0px 0px 10px rgba(0,0,0,0.1)';container.style.fontSize='14px';var listBtn=document.createElement('button');listBtn.innerText='List All Headings';listBtn.style.display='block';listBtn.style.marginBottom='10px';container.appendChild(listBtn);var closeBtn=document.createElement('button');closeBtn.innerText='Close';container.appendChild(closeBtn);document.body.appendChild(container);function listHeadings(doc,parentUl){var headings=doc.querySelectorAll('h1,h2,h3,h4,h5,h6');headings.forEach(function(heading){var item=document.createElement('li');item.innerText=heading.tagName+' - '+heading.innerText;parentUl.appendChild(item);});var iframes=doc.querySelectorAll('iframe');iframes.forEach(function(iframe){if(iframe.contentDocument){var iframeUl=document.createElement('ul');var iframeItem=document.createElement('li');iframeItem.innerText='Iframe ID: '+(iframe.id||'[No ID]');iframeItem.appendChild(iframeUl);parentUl.appendChild(iframeItem);listHeadings(iframe.contentDocument,iframeUl);}});}listBtn.onclick=function(){var list=document.createElement('ul');listHeadings(document,list);container.appendChild(list);};closeBtn.onclick=function(){document.body.removeChild(container);};})();
