	<script>
    document.addEventListener('DOMContentLoaded', function() {
      const links = document.querySelectorAll('a');
      const maxLength = 30;
      links.forEach(link => {
        const linkText = link.textContent;
        if (linkText.length > maxLength) {
          link.textContent = linkText.substring(0, maxLength) + '...';
        }
      });
    });
  </script>
	
